{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": [
    "svelte.config.js",
    "vite.config.js",
    "**/*.svelte"
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "no-console": [
      "off"
    ],
    "no-tabs": [
      "off"
    ],
    "space-unary-ops": [
      "off"
    ],
    "no-plusplus": [
      "off"
    ],
    "camelcase": [
      "off"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "eqeqeq": [
      "off"
    ],
    "max-len": [
      "off"
    ],
    "no-underscore-dangle": [
      "off"
    ],
    "func-names": [
      "off"
    ],
    "new-cap": [
      "off"
    ],
    "no-unused-vars": [
      "warn"
    ],
    "radix": [
      "off"
    ],
    "no-restricted-syntax": [
      "off"
    ],
    "no-loss-of-precision": [
      "warn"
    ],
    "guard-for-in": [
      "off"
    ],
    "prefer-destructuring": [
      "off"
    ],
    "no-await-in-loop": [
      "off"
    ],
    "no-restricted-globals": [
      "off"
    ],
    "no-alert": [
      "off"
    ],
    "no-continue": [
      "off"
    ],
    "no-lonely-if": [
      "off"
    ],
    "import/no-unresolved": [
      "warn"
    ]
  }
}
